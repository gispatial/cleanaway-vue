AAA,CAJqB,KAAA,CAAA,QAIrB,EAJ6B,UAAtB,OAAsB,EAAd,CAAc,EAAb;yBAAtB,YAAA,CAIM,KAJN,EAIM;AAJgC,MAAA,GAAG,EAAE,OAAO,CAAC,IAAR,GAAe,CAAC,CAAC,QAAF;AAIpD,KAJN,E,CACI,YAAA,CAAiD,MAAjD,EAAA,UAAA,EAAiD,gBAAA,CAAf,MAAA,CAAA,MAAe,CAAjD,EAAwC;AAAA;AAAxC,K,EACA,YAAA,CAAwD,MAAxD,EAAA,UAAA,EAAwD,gBAAA,CAArB,OAAO,CAAC,IAAa,CAAxD,EAA+C;AAAA;AAA/C,K,EACA,YAAA,CAA2D,KAA3D,EAAA,UAAA,EAA2D,gBAAA,CAAxB,OAAO,CAAC,QAAgB,CAA3D,EAAmD;AAAA;AAAnD,K,CAHJ,C;GAIM,CAJN,E;;AAAA,GAKE,EANN,C,EAOA,YAAA,CAGM,KAHN,EAAA,UAAA,EAGM,CAFF,YAAA,CAAiD,MAAjD,EAAA,UAAA,EAAiD,gBAAA,CAAf,MAAA,CAAA,MAAe,CAAjD,EAAwC;AAAA;AAAxC,GAEE,E,gBADF,YAAA,CAAsH,OAAtH,EAAsH;AAA/G,IAAA,GAAG,EAAC,OAA2G;AAAnG,IAAA,IAAI,EAAC,MAA8F;;aAA9E,KAAA,CAAA,WAAA,GAAW,M;MAAmE;AAAjE,IAAA,KAAK,EAAC,kBAA2D;AAAxC,IAAA,YAAY,EAAC,KAA2B;AAApB,IAAA,SAAO,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,SAAA,IAAA,QAAA,CAAA,SAAA,OAAA,QAAA,YAAF;AAAA,KAAA;AAAa,GAAtH,E,IAAA,E;;AAAA,G,iBAAwC,KAAA,CAAA,W,GACtC,CAHN,C,CATJ,C","sourcesContent":["<template>\n    <div class=\"p-terminal p-component\" @click=\"onClick\">\n        <div v-if=\"welcomeMessage\">{{welcomeMessage}}</div>\n        <div class=\"p-terminal-content\">\n            <div v-for=\"(command,i) of commands\" :key=\"command.text + i.toString()\">\n                <span class=\"p-terminal-prompt\">{{prompt}}</span>\n                <span class=\"p-terminal-command\">{{command.text}}</span>\n                <div class=\"p-terminal-response\">{{command.response}}</div>\n            </div>\n        </div>\n        <div class=\"p-terminal-prompt-container\">\n            <span class=\"p-terminal-prompt\">{{prompt}}</span>\n            <input ref=\"input\" type=\"text\" v-model=\"commandText\" class=\"p-terminal-input\" autocomplete=\"off\" @keydown=\"onKeydown\">\n        </div>\n    </div>\n</template>\n\n<script>\nimport TerminalService from 'primevue/terminalservice';\n\nexport default {\n    name: 'Terminal',\n    props: {\n        welcomeMessage: {\n            type: String,\n            default: null\n        },\n        prompt: {\n            type: String,\n            default: null\n        }\n    },\n    data() {\n        return {\n            commandText: null,\n            commands: []\n        }\n    },\n    mounted() {\n        TerminalService.on('response', this.responseListener);\n        this.$refs.input.focus();\n    },\n    updated() {\n        this.$el.scrollTop = this.$el.scrollHeight;\n    },\n    beforeUnmount() {\n        TerminalService.off('response', this.responseListener);\n    },\n    methods: {\n        onClick() {\n            this.$refs.input.focus();\n        },\n        onKeydown(event) {\n            if (event.keyCode === 13 && this.commandText) {\n                this.commands.push({text: this.commandText});\n                TerminalService.emit('command', this.commandText);\n                this.commandText = '';\n            }\n        },\n        responseListener(response) {\n            this.commands[this.commands.length - 1].response = response;\n        }\n    }\n}\n</script>\n\n<style>\n.p-terminal {\n    height: 18rem;\n    overflow: auto;\n}\n\n.p-terminal-prompt-container {\n    display: flex;\n    align-items: center;\n}\n\n.p-terminal-input {\n    flex: 1 1 auto;\n    border: 0 none;\n    background-color: transparent;\n    color: inherit;\n    padding: 0;\n    outline: 0 none;\n}\n\n.p-terminal-input::-ms-clear {\n    display: none;\n}\n</style>\n"],"sourceRoot":""}]}