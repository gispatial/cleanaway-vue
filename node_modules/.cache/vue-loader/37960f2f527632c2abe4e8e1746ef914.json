iv class=\"content-section introduction\">\n\t\t\t<div class=\"feature-intro\">\n\t\t\t\t<h1>Paginator</h1>\n\t\t\t\t<p>Paginator is a generic component to display content in paged format.</p>\n\t\t\t</div>\n            <AppDemoActions />\n\t\t</div>\n\n\t\t<div class=\"content-section implementation\">\n            <div class=\"card\">\n                <h5>Basic</h5>\n                <Paginator :rows=\"10\" :totalRecords=\"totalRecords\" :rowsPerPageOptions=\"[10,20,30]\"></Paginator>\n\n                <h5>Custom</h5>\n                <Paginator v-model:first=\"first\" :rows=\"1\" :totalRecords=\"totalRecords2\"\n                    template=\"FirstPageLink PrevPageLink CurrentPageReport NextPageLink LastPageLink\">\n                    <template #left>\n                        <Button type=\"button\" icon=\"pi pi-refresh\" @click=\"reset()\"/>\n                    </template>\n                    <template #right>\n                        <Button type=\"button\" icon=\"pi pi-search\" />\n                    </template>\n                </Paginator>\n\n                <div class=\"image-gallery\">\n                    <img :src=\"'demo/images/nature/' + image + '.jpg'\" />\n                </div>\n            </div>\n\t\t</div>\n\n        <PaginatorDoc />\n\t</div>\n</template>\n\n<script>\nimport PaginatorDoc from './PaginatorDoc';\n\nexport default {\n    data() {\n        return {\n\t\t\tfirst: 0,\n            totalRecords: 120,\n            totalRecords2: 12\n        }\n    },\n    methods: {\n        reset() {\n            this.first = 0;\n        }\n    },\n    computed: {\n        image() {\n            return 'nature' + (this.first + 1);\n        }\n    },\n    components: {\n        'PaginatorDoc': PaginatorDoc\n    }\n}\n</script>\n\n<style scoped>\n.p-button.p-button-icon-only {\n    border-radius: 0;\n}\n\n.image-gallery {\n    text-align: center;\n    padding: 1rem;\n}\n</style>"]}]}